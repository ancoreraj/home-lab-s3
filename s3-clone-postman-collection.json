{
	"info": {
		"_postman_id": "b7a29c9d-4352-4fe1-b8ec-4a039f9c1760",
		"name": "S3 Clone API",
		"description": "A collection for testing the S3 Clone API with all the core S3-like operations.",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Health Check",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}",
					"host": [
						"{{baseUrl}}"
					]
				},
				"description": "Check if the S3 Clone API is running"
			},
			"response": []
		},
		{
			"name": "PUT Object - Upload File",
			"request": {
				"method": "PUT",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "file",
							"type": "file",
							"src": "/path/to/your/file"
						}
					]
				},
				"url": {
					"raw": "{{baseUrl}}/{{bucket}}/{{key}}",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"{{bucket}}",
						"{{key}}"
					]
				},
				"description": "Upload a file to a specified bucket with a specific key"
			},
			"response": []
		},
		{
			"name": "GET Object - Download File",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/{{bucket}}/{{key}}",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"{{bucket}}",
						"{{key}}"
					]
				},
				"description": "Download a file from a specified bucket with a specific key"
			},
			"response": []
		},
		{
			"name": "LIST Bucket - List Files",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/{{bucket}}",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"{{bucket}}"
					]
				},
				"description": "List all files in a specified bucket"
			},
			"response": []
		},
		{
			"name": "DELETE Object - Delete File",
			"request": {
				"method": "DELETE",
				"header": [],
				"url": {
					"raw": "{{baseUrl}}/{{bucket}}/{{key}}",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"{{bucket}}",
						"{{key}}"
					]
				},
				"description": "Delete a file from a specified bucket with a specific key"
			},
			"response": []
		}
	],
	"event": [],
	"variable": [
		{
			"key": "baseUrl",
			"value": "http://localhost:3000",
			"type": "string",
			"description": "The base URL of your S3 Clone server"
		},
		{
			"key": "bucket",
			"value": "test-bucket",
			"type": "string",
			"description": "Name of the bucket"
		},
		{
			"key": "key",
			"value": "test-file.txt",
			"type": "string",
			"description": "Key/filename of the object"
		}
	]
}
